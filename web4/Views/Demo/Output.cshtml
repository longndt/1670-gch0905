@model web4.Models.Person

<div class="container col-md-5 text-center">
    <div class="row">
        <div class="col" colspan="2">
            <h1>Person Details</h1>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <h3>Person Id</h3>
        </div>
        <div class="col">
            <h3>@Model.Id</h3>
        </div>
    </div>
    
     <div class="row">
        <div class="col">
            <h3>Person Name</h3>
        </div>
        <div class="col">
            <h3>@Model.Name</h3>
        </div>
    </div>

     <div class="row">
        <div class="col">
            <h3>Person Email</h3>
        </div>
        <div class="col">
            <h3>@Model.Email</h3>
        </div>
    </div>

    <div class="row">
        <div class="col">
            <h3>Person Address</h3>
        </div>
        <div class="col">
            <h3>@Model.Address</h3>
        </div>
    </div>
</div>
