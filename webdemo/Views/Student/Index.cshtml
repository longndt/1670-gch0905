@model ICollection<webdemo.Models.Student>

<div class="container col-md-12 text-center mt-3">
    <h1 class="text text-success">Student List</h1>
    <table class="table table-primary mt-3">
        <tr>
            <th>Student Id</th>
            <th>Last Name</th>
            <th>First & Middle Name</th>
            <th>Enrollment Date</th>
            <th>Actions</th>
        </tr>
        @foreach (var item in Model)
        {
           <tr>
               <td>@item.Id</td>
               <td>@item.LastName</td>
               <td>@item.FirstMidName</td>
               <td>@item.EnrollmentDate</td>
               <td>
                   <a class="btn btn-info">Detail</a>
                   <a class="btn btn-warning">Edit</a>
                   <a class="btn btn-danger">Delete</a>
               </td>
           </tr>
        }
    </table>

</div>